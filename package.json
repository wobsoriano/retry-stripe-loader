{
  "name": "retry-stripe-loader",
  "version": "0.1.1",
  "packageManager": "pnpm@7.14.0",
  "description": "",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/retry-stripe-loader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/retry-stripe-loader.git"
  },
  "bugs": "https://github.com/wobsoriano/retry-stripe-loader/issues",
  "keywords": [
    "stripe",
    "retry",
    "payment",
    "loader"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm build-fast --dts-resolve",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stripe/stripe-js": "*"
  },
  "dependencies": {
    "p-retry": "^5.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@stripe/stripe-js": "^1.42.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "tsup": "6.4.0",
    "tsx": "^3.10.4",
    "typescript": "^4.7.4",
    "vitest": "^0.24.3"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
